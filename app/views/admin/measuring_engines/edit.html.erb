
<h1>Edition du moyen de mesure et d'analyse <%= @measuring_engine.name %></h1>
<% nested_form_for([:admin, @measuring_engine], :html => {:multipart => true}) do |f| %>
  <%= render :partial => "form", :locals => {:f => f} %>
  <p class="buttons">
    <%= submit_tag "Enregistrer", :class => "button" %>
     ou <%= link_to "Annuler", :back %>
  </p>
<% end %>
