<div style="width:50%;float:left">
  <h1>Edition de la calibration <%= @calibration.calibration %></h1>
  <% form_for([:admin, @calibration], :html => {:multipart => true}) do |f| %>
    <%= render :partial => "form", :locals => {:f => f} %>
    <p class="buttons">
      <%= submit_tag "Enregistrer", :class => "button" %>
    </p>
  <% end %>
</div>
