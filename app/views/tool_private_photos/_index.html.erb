<% if current_user && photos.size>0%>  
  <ul style="padding-left:0;">
    <% photos.each do |photo| %>
      <li style="float:left;margin:5px;list-style:none;height:100px;"><%= image_tag(photo.asset.url(:thumb), :title => photo.legend, :target => "_blank") %></li>
    <% end %>
  </ul>
  <br />
  <div style="clear:both"></div>
<% end %>
