<% if current_user && photos.size>0%>
  Photos non diffusables :<br />
  <ul style="padding-left:0;">
    <% photos.each do |photo| %>
      <li style="float:left;margin:5px;list-style:none;"><%= image_tag(photo.asset.url(:thumb)) %></li>
    <% end %>
  </ul>
  <br />
  <div style="clear:both"></div>
<% end %>
